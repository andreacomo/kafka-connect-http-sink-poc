{
  "value.converter.schema.registry.url": "http://schema-registry:8081",
  "name": "users-http-sink",
  "connector.class": "io.confluent.connect.http.HttpSinkConnector",
  "key.converter": "org.apache.kafka.connect.storage.StringConverter",
  "value.converter": "org.apache.kafka.connect.storage.StringConverter",
  "transforms": "Cast",
  "errors.retry.timeout": "-1",
  "errors.retry.delay.max.ms": "5000",
  "errors.tolerance": "all",
  "errors.log.enable": "true",
  "errors.log.include.messages": "true",
  "topics": "users",
  "errors.deadletterqueue.topic.name": "users.http.sink.dlq",
  "errors.deadletterqueue.topic.replication.factor": "1",
  "errors.deadletterqueue.context.headers.enable": "true",
  "transforms.Cast.type": "org.apache.kafka.connect.transforms.Cast$Key",
  "transforms.Cast.spec": "int32",
  "http.api.url": "http://host.docker.internal:8080/api/users",
  "request.method": "post",
  "behavior.on.error": "ignore",
  "headers": "Content-Type:application/json",
  "auth.type": "none",
  "max.retries": "10",
  "retry.backoff.ms": "1000",
  "retry.on.status.codes": "400-",
  "reporter.result.topic.name": "users.http.sink.success",
  "reporter.result.topic.replication.factor": "1",
  "reporter.error.topic.name": "users.http.sink.error",
  "reporter.error.topic.replication.factor": "1",
  "reporter.bootstrap.servers": "broker:29092",
  "confluent.topic.bootstrap.servers": "broker:29092"
}